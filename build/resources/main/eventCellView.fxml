<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="341.0" prefWidth="412.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="eventImage" fitHeight="233.0" fitWidth="421.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/defaultImage.png" />
         </image>
      </ImageView>
      <Label fx:id="eventDate" layoutX="22.0" layoutY="241.0" prefHeight="18.0" prefWidth="392.0" text="WED, DEC 29 AT 3:00 AM">
         <font>
            <Font name="Verdana Bold" size="13.0" />
         </font>
      </Label>
      <Label fx:id="eventName" layoutX="22.0" layoutY="266.0" prefHeight="26.0" prefWidth="372.0" text="EmTech Digital: An AI Conference">
         <font>
            <Font name="Verdana Bold" size="15.0" />
         </font>
      </Label>
      <Button fx:id="goingButton" alignment="BASELINE_LEFT" layoutX="95.0" layoutY="300.0" mnemonicParsing="false" prefHeight="28.0" prefWidth="78.0" style="-fx-background-color: #0000ff;" text="Going" textFill="WHITE">
         <graphic>
            <ImageView fx:id="goingImage" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true" />
         </graphic></Button>
      <Button fx:id="notificationButton" layoutX="208.0" layoutY="301.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="114.0" style="-fx-background-color: #0000ff;" text="Notifications: Off" textFill="WHITE" />
   </children>
</AnchorPane>
